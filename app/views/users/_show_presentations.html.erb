<div id="presentations" class="tab-pane in active profile_tab">
  <% if @isProfileOwner %>
    <%= render 'resources/resource_new', :new_resource_path => new_presentation_path %>
  <%end%>
  <% @presentations.each do |presentation| %>
    <%= render 'presentations/presentation', :presentation => presentation %>
  <% end %>
</div>
